<p id="notice"><%= notice %></p>

<h1>Resultado de busca</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Nome</th>
      <th>Nacionalidade</th>
      <th>E-mail</th>
      <th>Senha</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.username %></td>    
        <td><%= usuario.nome %></td>
        <td><%= usuario.nacionalidade %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.senha %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', '/buscar' %>
